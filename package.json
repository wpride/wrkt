{
  "name": "wrkt",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": "8.11.1"
  },
  "scripts": {
    "lint": "eslint .",
    "prestart": "npm run --prefix client build",
    "start": "node server/server.js",
    "start-dev": "concurrently \"nodemon .\" \"npm start --prefix client\"",
    "posttest": "npm run lint &amp;&amp; nsp check",
    "heroku-postbuild": "webpack -p",
    "client": "cd client && npm webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon",
    "dev": "concurrently \"npm run server\" \"cd client && npm run dev\""
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "compression": "^1.0.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "helmet": "^3.10.0",
    "loopback": "^3.22.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^6.2.0",
    "loopback-console": "^1.1.0",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^3.0.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0",
    "nodemon": "^1.18.9"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "wrkt",
  "proxy": {
    "/api": {
      "target": "http://localhost:3000"
    }
  }
}
